{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "text2graph",
  "title": "QueryGPT",
  "description": "",
  "icon": "command-icon.png",
  "author": "mdolr",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "QueryGPT",
      "description": "Template for submitting a form",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "type": "password",
      "description": "OpenAI API Key",
      "name": "openai_api_key",
      "title": "OpenAI API Key",
      "required": true
    },
    {
      "type": "password",
      "description": "Postgres Database URL",
      "name": "postgres_url",
      "title": "Postgres Database URL",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.8",
    "canvas": "^2.11.0",
    "chart.js": "^4.2.1",
    "chart.js-image": "^6.1.3",
    "openai": "^3.2.1",
    "pg": "^8.10.0",
    "pg-native": "^3.0.1",
    "postgres": "^3.3.4",
    "react-chartjs-2": "^5.2.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
